{
  "name": "zoneless",
  "version": "0.1.0-beta.1",
  "description": "Zoneless SDK for Node.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "zoneless",
    "usdc",
    "solana",
    "crypto",
    "payments",
    "connect",
    "payouts",
    "sdk"
  ],
  "author": "Zoneless",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zonelessdev/zoneless-node.git"
  },
  "homepage": "https://zoneless.com/docs",
  "bugs": {
    "url": "https://github.com/zonelessdev/zoneless-node/issues"
  },
  "dependencies": {
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

